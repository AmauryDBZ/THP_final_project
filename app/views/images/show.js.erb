let image = $("#modal-image").find(".modal-content").html("<%= j (render 'fullscreen') %>");
// let image = document.getElementById('modal-content-image').html("<%= j (render 'fullscreen') %>");

function dbl_click(){
  image.addEventListener("click", image_fullscreen);
  function image_fullscreen(){
    $("#modal-image").modal();
  };
};

$("#modal-image").modal();
image.addEventListener("click", dbl_click());
